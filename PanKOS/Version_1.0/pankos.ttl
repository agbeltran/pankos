#################################################################
#
#    Annotation properties
#
#################################################################


###  http://www.purl.org/pankos#preferredName

pankos:preferredName rdf:type :AnnotationProperty .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#hasOperationStatus

<http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#hasOperationStatus> rdf:type :FunctionalProperty ,
                                                                                        :ObjectProperty ;
                                                                               
                                                                               rdfs:range <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#StatusValuePartition> ;
                                                                               
                                                                               rdfs:subPropertyOf :topObjectProperty .



###  http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#hasPressureLevel

<http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#hasPressureLevel> rdf:type :ObjectProperty ;
                                                                             
                                                                             rdfs:range <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#PressureLevelValuePartition> ;
                                                                             
                                                                             rdfs:subPropertyOf :topObjectProperty .



###  http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#usesFileFormat

<http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#usesFileFormat> rdf:type :ObjectProperty ;
                                                                           
                                                                           rdfs:domain pankos:Facility .



###  http://www.purl.org/pankos#hasEnergyLevel

pankos:hasEnergyLevel rdf:type :ObjectProperty .



###  http://www.purl.org/pankos#hasInstrument

pankos:hasInstrument rdf:type :AsymmetricProperty ,
                              :ObjectProperty ;
                     
                     rdfs:domain pankos:Facility ;
                     
                     rdfs:range pankos:Instrument ;
                     
                     :inverseOf pankos:inFacility ;
                     
                     rdfs:subPropertyOf :topObjectProperty .



###  http://www.purl.org/pankos#hasResolutionLevel

pankos:hasResolutionLevel rdf:type :FunctionalProperty ,
                                   :ObjectProperty ;
                          
                          rdfs:range pankos:ValuePartition .



###  http://www.purl.org/pankos#hasSubFacility

pankos:hasSubFacility rdf:type :ObjectProperty .



###  http://www.purl.org/pankos#inFacility

pankos:inFacility rdf:type :AsymmetricProperty ,
                           :ObjectProperty ;
                  
                  rdfs:range pankos:Facility ;
                  
                  rdfs:domain pankos:Instrument .



###  http://www.purl.org/pankos#supportsTechnique

pankos:supportsTechnique rdf:type :AsymmetricProperty ,
                                  :ObjectProperty ;
                         
                         rdfs:domain pankos:Instrument ;
                         
                         rdfs:range pankos:Technique .



###  http://www.purl.org/pankos#techniqueOf

pankos:techniqueOf rdf:type :AsymmetricProperty ,
                            :ObjectProperty ;
                   
                   rdfs:range pankos:Instrument ;
                   
                   rdfs:domain pankos:Technique ;
                   
                   :inverseOf pankos:supportsTechnique .





