    <NamedIndividual rdf:about="&pankos;BM28">
        <rdf:type rdf:resource="&pankos;Instrument"/>
        <pankos:preferredName rdf:datatype="&xsd;string">BM28</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">XMaS is a materials research beamline mainly dedicated to magnetic materials. It offers a wide range of techniques and sample environments facilitating studies of soft and hard condensed matter, physical chemistry, and solid and liquid interfaces.</dc:description>
