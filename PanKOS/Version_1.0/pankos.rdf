    <!-- http://www.purl.org/pankos#PLEIADES -->

    <NamedIndividual rdf:about="&pankos;PLEIADES">
        <pankos:preferredName rdf:datatype="&xsd;string">PLEIADES</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">PLEIADES is an ultra high resolution soft X-ray beamline (ultimate resolving power of about 100000 at 50 eV) covering the energy domain ranging from 10 eV to 1 keV. PLEIADES is dedicated to spectroscopy based atomic and molecular physics studies of diluted samples (atoms, molecules, ions, clusters, molecules adsorbed on surfaces).</dc:description>
        <pankos:supportsTechnique rdf:resource="&protege-dc;MassSpectrometry"/>
        <pankos:inFacility rdf:resource="&pankos;SOLEIL"/>
        <pankos:supportsTechnique rdf:resource="&pankos;XAS"/>
