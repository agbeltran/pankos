        <pankos:preferredName rdf:datatype="&xsd;string">SALSA</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">SALSA is the ILL strain imager dedicated to the determination of residual stresses in a broad range of applications in terms of components and materials. It is designed for diffraction measurements in &quot;real&quot; engineering components and optimised for stress determination in metallic components.</dc:description>
        <rdfs:comment rdf:datatype="&xsd;string">Strain imager for engineering applications SALSA</rdfs:comment>
        <pankos:inFacility rdf:resource="&pankos;ILL"/>
        <pankos:supportsTechnique rdf:resource="&pankos;PowderDiffraction"/>
    </NamedIndividual>
    


    <!-- http://www.purl.org/pankos#SAMBA -->

    <NamedIndividual rdf:about="&pankos;SAMBA">
        <pankos:preferredName rdf:datatype="&xsd;string">SAMBA</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">SAMBA is one of the two hard X-ray absorption spectroscopy (XAS) beamlines at SOLEIL together with Diffabs. SAMBA aiming to be dedicated to large and varied scientific communities from physics to chemistry and biologY. The design of SAMBA optics is optimised in order to be very versatile, to cover permanently the 4-42 keV energy range with a high flux of photon and a good energy resolution.</dc:description>
        <pankos:supportsTechnique rdf:resource="&protege-dc;DifferentialScanningCalorimetry"/>
        <pankos:supportsTechnique rdf:resource="&protege-dc;UV-VisibleSpectroscopy"/>
        <pankos:supportsTechnique rdf:resource="&pankos;RamanSpectroscopy"/>
        <pankos:inFacility rdf:resource="&pankos;SOLEIL"/>
    </NamedIndividual>
    


