    <!-- http://www.purl.org/pankos#BM29 -->

    <NamedIndividual rdf:about="&pankos;BM29">
        <pankos:preferredName rdf:datatype="&xsd;string">BM29</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">The BioSAXS beamline is a highly automated beamline dedicated to the study of proteins, macromolecular complexes, viruses etc., in solution. Samples can be investigated under various conditions (temperature, buffer, pH, kinetics) in a high-throughput manner or a HPLC unit can be used for in-situ (online) purification.

</dc:description>
        <pankos:inFacility rdf:resource="&pankos;ESRF"/>
        <pankos:supportsTechnique rdf:resource="&pankos;SAXS"/>
    </NamedIndividual>
    


    <!-- http://www.purl.org/pankos#BM30A -->

    <NamedIndividual rdf:about="&pankos;BM30A">
        <pankos:preferredName rdf:datatype="&xsd;string">BM30A</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">This beamline is specially dedicated to crystallography of biological macromolecules with normal or multi-wavelength diffraction using anomalous dispersion.</dc:description>
        <pankos:inFacility rdf:resource="&pankos;ESRF"/>
        <pankos:supportsTechnique rdf:resource="&pankos;MAD"/>
        <pankos:supportsTechnique rdf:resource="&pankos;MacromolecularCrystallography"/>
