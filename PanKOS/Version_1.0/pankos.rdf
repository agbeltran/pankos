    <!-- http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#ZOOM -->

    <NamedIndividual rdf:about="&protege-dc;ZOOM">
        <pankos:preferredName rdf:datatype="&rdfs;Literal">ZOOM</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">Zoom will be a flexible, high count rate small-angle scattering instrument for advanced materials, magnetism, environment science, pharmacy and healthcare to study length scales 2‑2000 nm. For the first time at ISIS, it will offer polarised small angle neutron scattering and will use novel focusing devices and high-resolution detectors to reach smaller Q, to complement the Sans2d instrument, without building a very long beam line. Zoom will start commissioning experiments in February 2015.</dc:description>
        <protege-dc:hasOperationStatus rdf:resource="&protege-dc;UnderConstruction"/>
        <pankos:supportsTechnique rdf:resource="&protege-dc;VSANS"/>
