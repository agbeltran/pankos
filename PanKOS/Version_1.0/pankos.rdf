    <!-- http://www.purl.org/pankos#BM14 -->

    <NamedIndividual rdf:about="&pankos;BM14">
        <pankos:preferredName rdf:datatype="&xsd;string">BM14</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">BM14 is a macromolecular crystallography beamline specializing in the multiple and single anomalous diffraction (MAD/SAD) methods that enable users to determine the 3D atomic structure of crystalline macro-molecules.</dc:description>
        <pankos:inFacility rdf:resource="&pankos;ESRF"/>
        <pankos:supportsTechnique rdf:resource="&pankos;MAD"/>
        <pankos:supportsTechnique rdf:resource="&pankos;MacromolecularCrystallography"/>
    </NamedIndividual>
    


