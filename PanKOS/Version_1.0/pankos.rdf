        <pankos:preferredName rdf:datatype="&xsd;string">IN15</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">IN15 is a high energy and momentum resolution spin-echo spectrometer optimised for quasielastic small angle scattering. This instrument is developed and financed jointly by the ILL, FZ Jülich and HMI Berlin.</dc:description>
        <rdfs:comment rdf:datatype="&xsd;string">Spin-echo spectrometer with time-of-flight option and focussing option</rdfs:comment>
        <pankos:hasResolutionLevel rdf:resource="&pankos;HighResolution"/>
