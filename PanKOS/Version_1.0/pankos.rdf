    <!-- http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#EXEED -->

    <NamedIndividual rdf:about="&protege-dc;EXEED">
        <pankos:preferredName rdf:datatype="&rdfs;Literal">EXEED</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">Exeed will be a neutron time-of-flight diffractometer optimised for extreme environment studies of materials which will complement the capabilities of Wish on TS2 and Pearl on TS1. Exeed will deliver an extremely bright, focused beam in the thermal-cold region, providing world-class access to regions of the phase diagram that have so far eluded neutron studies. </dc:description>
        <protege-dc:hasOperationStatus rdf:resource="&protege-dc;UnderProposal"/>
        <pankos:inFacility rdf:resource="&pankos;ISIS"/>
        <pankos:supportsTechnique rdf:resource="&pankos;Time-of-FlightDiffraction"/>
