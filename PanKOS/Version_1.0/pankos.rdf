    <!-- http://www.purl.org/pankos#IN10 -->

    <NamedIndividual rdf:about="&pankos;IN10">
        <rdf:type rdf:resource="&pankos;BackScatteringSpectrometer"/>
        <rdfs:comment rdf:datatype="&xsd;string">Cold neutron backscattering spectrometer IN10</rdfs:comment>
        <pankos:preferredName rdf:datatype="&xsd;string">IN10</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">The IN10 backscattering spectrometer is designed for inelastic or quasielastic scattering experiments requiring very high energy resolution and moderate momentum transfer resolution. IN10 owes its high energy resolution to the use of nearly perfect backscattering both at the monochromator and at the analyser crystals.</dc:description>
        <pankos:supportsTechnique rdf:resource="&pankos;ElasticNeutronScatteringSpectroscopy"/>
        <pankos:hasResolutionLevel rdf:resource="&pankos;HighResolution"/>
        <pankos:inFacility rdf:resource="&pankos;ILL"/>
        <pankos:supportsTechnique rdf:resource="&pankos;InelasticNeutronScatteringSpectroscopy"/>
    </NamedIndividual>
    


    <!-- http://www.purl.org/pankos#IN11 -->

    <NamedIndividual rdf:about="&pankos;IN11">
        <rdf:type rdf:resource="&pankos;Spin-echoSpectrometer"/>
        <pankos:preferredName rdf:datatype="&xsd;string">IN11</pankos:preferredName>
        <rdfs:comment rdf:datatype="&xsd;string">Spin-Echo Spectrometer IN11</rdfs:comment>
        <dc:description rdf:datatype="&xsd;string">The instrument IN11 is primarily used to study slow relaxation phenomena in polymers, glasses or magnetic materials. Other applications are elastic paramagnetic scattering and the determination of phonon linewidths.</dc:description>
        <pankos:hasResolutionLevel rdf:resource="&pankos;HighResolution"/>
        <pankos:inFacility rdf:resource="&pankos;ILL"/>
        <pankos:supportsTechnique rdf:resource="&pankos;SpinEchoSANS"/>
