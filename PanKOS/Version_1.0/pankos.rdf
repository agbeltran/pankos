    <!-- http://www.purl.org/pankos#B22 -->

    <NamedIndividual rdf:about="&pankos;B22">
        <pankos:preferredName rdf:datatype="&xsd;string">B22</pankos:preferredName>
        <dc:description rdf:datatype="&xsd;string">MIRIAM (Multimode InfraRed Microspectroscopy And Imaging) provides a brilliant and versatile microprobe for high resolution imaging of molecular structures as well as high sensitivity vibrational spectroscopy up to the THz range. Experiments span from biomedical applications to physical-chemical researches, with subsequent impact across a wide range of life and physical sciences.</dc:description>
        <pankos:supportsTechnique rdf:resource="&protege-dc;Infrared"/>
        <pankos:inFacility rdf:resource="&pankos;DIAMOND"/>
        <pankos:supportsTechnique rdf:resource="&pankos;Imaging"/>
        <pankos:supportsTechnique rdf:resource="&pankos;InfraredMicroSpectroscopy"/>
        <pankos:supportsTechnique rdf:resource="&pankos;Spectroscopy"/>
    </NamedIndividual>
    


