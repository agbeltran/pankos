    </NamedIndividual>
    


    <!-- http://www.purl.org/pankos#IN13 -->

    <NamedIndividual rdf:about="&pankos;IN13">
        <rdf:type rdf:resource="&pankos;NeutronSpectrometer"/>
        <dc:description rdf:datatype="&xsd;string">Because of its high energy resolution and high momentum transfer the backscattering spectrometer IN13 (CRG) is particularly useful for the microscopic study of single particle motions (jump reorientation, rotational and translational diffusion, tunnelling) observed by incoherent neutron scattering.</dc:description>
        <rdfs:comment rdf:datatype="&xsd;string">CRG - thermal neutron backscattering spectrometer IN13</rdfs:comment>
        <pankos:preferredName rdf:datatype="&xsd;string">IN13</pankos:preferredName>
        <pankos:supportsTechnique rdf:resource="&protege-dc;BackScatteringSpectroscopy"/>
        <pankos:hasResolutionLevel rdf:resource="&pankos;HighResolution"/>
        <pankos:inFacility rdf:resource="&pankos;ILL"/>
        <pankos:supportsTechnique rdf:resource="&pankos;ToFSpectroscopy"/>
    </NamedIndividual>
    


